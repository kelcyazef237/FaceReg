# Copy to .env and fill in values
# Generate keys with:  openssl rand -hex 32
SECRET_KEY=change_me_run_openssl_rand_hex_32
REFRESH_SECRET_KEY=change_me_refresh_run_openssl_rand_hex_32
DEBUG=false
DATABASE_URL=sqlite:///./facereg.db
# SFace cosine similarity threshold (0.593 recommended by OpenCV)
SIMILARITY_THRESHOLD=0.593
ACCESS_TOKEN_EXPIRE_MINUTES=15
REFRESH_TOKEN_EXPIRE_DAYS=7
MAX_UPLOAD_MB=10
